        android:src="@drawable/rectangle"></ImageView>

    <ImageView
        android:id="@+id/Box10"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:clickable="true"
        android:src="@drawable/rectangle"
        android:layout_alignTop="@+id/Box9"
