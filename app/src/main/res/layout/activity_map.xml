    <ImageView
        android:id="@+id/Box7"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/Box15"
        android:layout_alignTop="@+id/Box6"
        android:clickable="true"
        android:src="@drawable/rectangle"></ImageView>

    <ImageView
        android:id="@+id/Box9"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:clickable="true"
        android:src="@drawable/rectangle"
        android:layout_below="@+id/Box6"
        android:layout_alignStart="@+id/rectimage2"></ImageView>

    <ImageView
        android:id="@+id/Box16"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/Box8"
        android:layout_alignTop="@+id/Box15"
        android:clickable="true"
        android:src="@drawable/rectangle"></ImageView>

    <ImageView
        android:id="@+id/Box14"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:clickable="true"
        android:src="@drawable/rectangle"
        android:layout_alignTop="@+id/Box13"
        android:layout_alignStart="@+id/Box10"></ImageView>

    <ImageView
        android:id="@+id/Box3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/Box11"
        android:layout_alignTop="@+id/Box2"
        android:clickable="true"
        android:src="@drawable/rectangle"></ImageView>

    <ImageView
        android:id="@+id/rectimage2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:clickable="true"
        android:src="@drawable/rectangle"
        android:layout_below="@+id/Box2"
        android:layout_toStartOf="@+id/Box6"></ImageView>

</RelativeLayout>